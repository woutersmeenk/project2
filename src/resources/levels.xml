<levels>
    <level x="0" y="0" z="0">
        <cubes>
            <cube x="0" y="1" z="0" />
            <cube x="2" y="1" z="0" />
            <cube x="0" y="2" z="0" />
            <cube x="1" y="0" z="0" />
            <cube x="1" y="3" z="0" />
            <cube x="2" y="2" z="0">
                <switch>
                    <state>
                        <cube x="1" y="1" z="0" />
                    </state>
                    <state>
                        <cube x="1" y="2" z="0" />
                    </state>
                </switch>
            </cube>
        </cubes>
        <checkpoints>
            <checkpoint x="1" y="3" z="1" />
        </checkpoints>
        <start x="1" y="0" z="1" />
        <end   x="1" y="0" z="1" />
    </level>
    
<!-- level 2 -->
    <level x="5" y="0" z="0">
        <cubes>
            <cube x="0" y="-1" z="0" />
            <cube x="2" y="-1" z="0" />
            <cube x="0" y="-2" z="0" />
            <cube x="1" y="0" z="0" />
            <cube x="1" y="-3" z="0" />
            <cube x="2" y="-2" z="0">
                <switch>
                    <state>
                        <cube x="1" y="-1" z="0" />
                    </state>
                    <state>
                        <cube x="1" y="-2" z="0" />
                    </state>
                </switch>
            </cube>
        </cubes>
        <checkpoints>
            <checkpoint x="1" y="-3" z="1" />
        </checkpoints>
        <start x="1" y="0" z="1" />
        <end   x="1" y="0" z="1" />
    </level>
</levels>
